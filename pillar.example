slurm:
  ClusterName: "clustername"
  ControlMachine: '<Master slurm server>'
  ControlAddr: <ip slurm server>
  BackupController: '<slave slurm server>'
  BackupAddr: <ip slurm server>
  CheckpointType: 'blcr'
  AuthType: 'munge'
  CryptoType: 'munge'
  AccountingStorageType: slurmdbd
  TaskPlugin: 'cgroup'
  SlurmctldLogFile: '/var/log/slurm/slurmctld.log'
  AccountingStorageHost: '<slurm accounting server>'
  AccountingStoragePass: 'slurm accounting passwd'  
  AccountingStorageUser: 'slurm accountig user'
  SlurmDBStorageType: 'database type'
  PropagateResourceLimitsExcept: 'MEMLOCK'
  JobCompLoc: /var/log/slurm/slurm_jobcomp.log
  PrivateData: "usage"
  MaxArraySize: 100001
  MaxJobCount : 5000001
  PriorityType: 'multifactor'
  ProctrackType: 'cgroup'
  MailProg: '/bin/mail'
  AcctGatherEnergyType: 'ipmi'
  Frontends:
    frontend01:
      State: UNKNOWN
      AllowGroups: group alowed
      AllowUsers: <users alowed>
  nodes:
    clnode001:
      CPUS: 16
      RealMemory: 32
      Sockets: 2
      CoresPerSocket: 8
      ThreadsPerCore: 2
      State: UNKNOWN
      TmpDisk: 500000
      Feature: HyperThread
    clnode002:
      CPUS: 32
      RealMemory: 128
      Sockets: 2
      CoresPerSocket: 8
      ThreadsPerCore: 2
      State: UNKNOWN
      TmpDisk: 500000
      Feature: HyperThread
  partitions:
    fast:
      MaxTime: 01:00:00
      Default: 'No'
      State: UP
      Shared: 'YES'
      nodes:
        -clnode001
        -clnode002
    short:
      MaxTime: 06:00:00
      Default: 'yes'
      State: UP
      Shared: 'YES'
      nodes:
        - clnode001
    medium:
      MaxTime: 02-00:00:00
      Default: 'No'
      State: UP
      Shared: 'YES'
      nodes:
        - clnode002
